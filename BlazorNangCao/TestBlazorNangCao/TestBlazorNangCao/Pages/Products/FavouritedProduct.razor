@page "/favourites"
@inherits FavouritedProductBase

<h3>Favourited Products</h3>

<input type="text" class="input-group-sm" @bind="SearchString" placeholder="search something" />

<div class="wrapper">
    @foreach (var prod in FavouritedProducts)
    {
        <ShowProduct Prod="prod"></ShowProduct>
    }
</div>

